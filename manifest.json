{
  "name": "tabbi",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "storage"
],
  "chrome_url_overrides" : {
    "newtab": "new_page.html"
  },
  "content_scripts":[
    {
      "matches": ["<all_urls>"],
      "js":["content.js"]
    }
  ],
  "browser_action":{
    "default_popup":"popup.html",
    "default_title":"tabbi"
  }
}
